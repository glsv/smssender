{
    "name": "glsv/yii2-smssender",
    "description": "SMS sender via different SMS providers with log entires to the database.",
    "keywords": ["yii2", "sms", "sms-sender"],
    "type": "yii2-extension",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "glsv",
            "email": "glsv@yandex.ru"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "yiisoft/yii2": "~2.0.0",
        "33evgenyy33/yii2-smsaerov2": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "glsv\\smssender\\": "src"
        }
    },
    "extra": {
        "bootstrap": "glsv\\smssender\\Bootstrap"
    }
}
